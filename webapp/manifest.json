{
    "_version": "1.17.0",
    "sap.app": {
        "id": "project1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.15.7",
            "toolsId": "39e37090-ec69-406c-bc9f-a50b8ae4f5ad"
        },
        "dataSources": {
            "mainService": {
                "uri": "/V3/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.71.72",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "project1.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "jsonData": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/jsonData.json"
            },
            "productsData": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/productsData.json"
            },
            "data": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/data.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "project1.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": ":?query:",
                    "target": [
                        "TargetView1"
                    ]
                },
                {
                    "name": "View1",
                    "pattern": "View1",
                    "target": "View1"
                },
                {
                    "name": "helloWorld",
                    "pattern": "helloWorld",
                    "target": "helloWorld"
                },
                {
                    "name": "CssVariables",
                    "pattern": "CssVariables",
                    "target": "CssVariables"
                },
                {
                    "name": "JsObject",
                    "pattern": "JsObject",
                    "target": "JsObject"
                },
                {
                    "name": "JsonModel",
                    "pattern": "JsonModel",
                    "target": "JsonModel"
                },
                {
                    "name": "TwoWayBinding",
                    "pattern": "TwoWayBinding",
                    "target": "TwoWayBinding"
                },
                {
                    "name": "InputList",
                    "pattern": "InputList",
                    "target": "InputList"
                },
                {
                    "name": "Form",
                    "pattern": "Form",
                    "target": "Form"
                },
                {
                    "name": "ProductList",
                    "pattern": "ProductList",
                    "target": "ProductList"
                },
                {
                    "name": "ProductDetails",
                    "pattern": "ProductDetails",
                    "target": "ProductDetails"
                },
                {
                    "name": "ComboBox",
                    "pattern": "ComboBox",
                    "target": "ComboBox"
                }

            ],
            "targets": {
                "TargetView1": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "View1",
                    "viewName": "View1"
                },
                "helloWorld": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "helloWorld",
                    "viewName": "helloWorld"
                },
                "CssVariables": {
                   "viewType": "XML",
                    "transition": "slide",
                    "viewId": "CssVariables",
                    "viewName": "CssVariables" 
                },
                "JsObject": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "JsObject",
                    "viewName": "JsObject"
                },
                "JsonModel": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "JsonModel",
                    "viewName": "JsonModel"
                },
                "TwoWayBinding": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "TwoWayBinding",
                    "viewName": "TwoWayBinding"
                },
                "InputList": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "InputList",
                    "viewName": "InputList"
                },
                "Form": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "Form",
                    "viewName": "Form"
                },
                "ProductList": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "ProductList",
                    "viewName": "ProductList"
                },
                "ProductDetails": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "ProductDetails",
                    "viewName": "ProductDetails"
                },
                "ComboBox":{
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "ComboBox",
                    "viewName": "ComboBox"
                }
            }
        },
        "rootView": {
            "viewName": "project1.view.App",
            "type": "XML",
            "async": true,
            "id": "App"

        }
    }
}